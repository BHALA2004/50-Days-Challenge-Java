public class Solution {
    public static boolean palindromeNumber(int n){
        // Write your code here.
        int r=0,s=0,t=n;
        while(n>0){
            r=n%10;
            s=s*10+r;
            n=n/10;
        }
        if(t==s)
            return true;
        else
            return false;
    }
}
